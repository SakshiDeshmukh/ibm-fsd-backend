<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
   <display-name> Web Application</display-name>
  
  <servlet>
    <servlet-name>EmployeeRegister</servlet-name>
    <servlet-class>com.examples.java.servlet.jsp.Register</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EmployeeRegister</servlet-name>
    <url-pattern>/register.do</url-pattern>
  </servlet-mapping>
    <servlet>
    <servlet-name>EmployeeLogin</servlet-name>
    <servlet-class>com.examples.java.servlet.jsp.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EmployeeLogin</servlet-name>
    <url-pattern>/login.do</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>EmployeeAdd</servlet-name>
    <servlet-class>com.examples.java.servlet.jsp.Add</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EmployeeAdd</servlet-name>
    <url-pattern>/add.do</url-pattern>
  </servlet-mapping>
   
  
</web-app>
